    'mob_detector.pt': 'https://drive.google.com/uc?id=YOUR_MODEL_ID',
    'block_detector.pt': 'https://drive.google.com/uc?id=YOUR_MODEL_ID'
}

for name, url in model_urls.items():
    output = f'models/{name}'
    if not os.path.exists(output):
        print(f'Downloading {name}...')
        gdown.download(url, output, quiet=False)

